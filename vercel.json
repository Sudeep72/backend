{
  "buildCommand": "python app.py",
  "env": {
    "PYTHON_VERSION": "3.10"
  },
  "framework": {
    "name": "python",
    "version": "3.10"
  },
  "functions": {
    "api/predict": {
      "runtime": "python",
      "environment": {
        "FLASK_APP": "handler.py",
        "FLASK_ENV": "production",
        "PYTHON_VERSION": "3.10"
      }
    }
  },
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/predict"
    }
  ]
}
